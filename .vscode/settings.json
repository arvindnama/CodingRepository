{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        ".svn":true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.js.map":true,
        "**/*.sln":true,
        "**/*.csproj":true,
        "**/*.csproj.user":true,
        "cobertura-coverage.xml":true,
        "dojo-changes.txt":true,
        "jshint*.txt":true,
        "*.ico":true,
        "web*.config":true,
        "README.txt":true,
        "version.html":true,
        "showlogs.html":true,
        "canvasonlydeployment":true,
        "Deployment":true,
        "DeploymentManifestFiles":true,
        "html-report":true,
        "obj":true,
        "node_modules":true,
        "util":true,
        "bin":true,
        "tools":true,
        "dojo":false,
        "dijit":true,
        "dojox":true,
        "external":false,
        "external-amd":true,
        ".vs":true
    },
    "jshint.options": {
        "esversion": 6
    },
    "editor.renderWhitespace": "all",
    "typescript.referencesCodeLens.enabled": false
  }